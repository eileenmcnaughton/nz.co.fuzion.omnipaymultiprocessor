{
    "name": "omnipay/mollie",
    "type": "library",
    "description": "Mollie driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "mollie",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-mollie",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-mollie/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Mollie\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\Mollie\\Test\\": "tests/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3.1"
    },
    "require-dev": {
        "omnipay/tests": "^4.1",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.5.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true
}
